# Minecraft Account Settings
MC_USERNAME=YourMinecraftUsername
MC_PASSWORD=YourMinecraftPassword
MC_AUTH=offline

# Server Connection Settings
MC_HOST=localhost
MC_PORT=25565
MC_VERSION=1.21

# Bot Behavior Settings
AUTO_RESPAWN=true
ANTI_KICK=true
CHAT_DELAY=1000
MOVE_TIMEOUT=10000

# Auto-login Settings
AUTO_LOGIN=true
LOGIN_PASSWORD=botminecraft
LOGIN_DELAY=3000

# Anti-kick Settings
ANTI_KICK_INTERVAL=30000

# Follow Settings
FOLLOW_DISTANCE=3.0

# Pathfinding Settings
PATHFINDING_TIMEOUT=10000

# Logging Settings
LOG_LEVEL=info
LOG_FILE=bot.log
NODE_ENV=development
